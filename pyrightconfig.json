{
    "include": [
        "src",
        "tests"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.*"
    ],
    "pythonVersion": "3.8",
    "pythonPlatform": "All",
    "typeCheckingMode": "basic",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonPath": "venv/bin/python",
    "venvPath": ".",
    "venv": "venv"
} 